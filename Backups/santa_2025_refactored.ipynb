    "import importlib\n",
    "import optimization_utils\n",
    "importlib.reload(optimization_utils)\n",
    "from optimization_utils import sa_numba_growing\n",
    "from joblib import Parallel, delayed\n",
    "import multiprocessing\n",
    "\n",
    "def optimize_packing(trees, params):\n",
    "    if not trees: return trees\n",
    "    n = len(trees)\n",
    "    xs = np.array([t.center_x for t in trees], dtype=np.float64)\n",
    "    ys = np.array([t.center_y for t in trees], dtype=np.float64)\n",
    "    angs = np.array([t.angle for t in trees], dtype=np.float64)\n",
    "    p = params\n",
    "    best_xs, _, _, _ = sa_numba_growing(xs, ys, angs, n, p['iterations'], p['initial_temp'], p['final_temp'], p['step_size'], p['angle_step'], np.random.randint(0, 1000000), p['compression'])\n",
    "    for i in range(n): trees[i].update_position(best_xs[i], ys[i], angs[i])\n",
    "    return center_packing(trees)\n",
    "\n",
    "def run_pipeline():\n",
    "    known_solutions = {}\n",
    "    try:\n",
    "        df = pd.read_csv('test.csv')\n",
    "        df['x'] = df['x'].apply(lambda v: float(str(v).replace('s', '')))\n",
    "        df['y'] = df['y'].apply(lambda v: float(str(v).replace('s', '')))\n",
    "        df['deg'] = df['deg'].apply(lambda v: float(str(v).replace('s', '')))\n",
    "        for n in range(1, 201):\n",
    "            rows = df[df['id'].str.startswith(f\"{n:03d}_\")]\n",
    "            if len(rows) == n: known_solutions[n] = [ChristmasTree(row['x'], row['y'], row['deg']) for _, row in rows.iterrows()]\n",
    "        print(f\"Loaded {len(known_solutions)} baselines.\")\n",
    "    except Exception as e: print(f\"Could not load baseline: {e}\")\n",
    "    \n",
    "    all_solutions = {}\n",
    "    for n in tqdm(range(200, 0, -1), desc=\"Processing\"):\n",
    "        params = get_sa_params(n)\n",
    "\n",
    "        if n + 1 in all_solutions:\n",
    "            trees = [ChristmasTree(t.center_x, t.center_y, t.angle) for t in all_solutions[n+1]]\n",
    "            trees.pop(np.random.randint(0, len(trees)))\n",
    "        elif n in known_solutions:\n",
    "            trees = known_solutions[n]\n",
    "        else:\n",
    "            trees = [ChristmasTree(np.random.randn(), np.random.randn(), np.random.uniform(0, 360)) for _ in range(n)]\n",
    "            \n",
    "        optimized_trees = optimize_packing(trees, params)\n",
    "        is_valid, _ = validate_no_collisions(optimized_trees)\n",
    "        if not is_valid and n in known_solutions:\n",
    "             is_baseline_valid, _ = validate_no_collisions(known_solutions[n])\n",
    "             if is_baseline_valid: optimized_trees = known_solutions[n] # Revert\n",
    "        all_solutions[n] = optimized_trees\n",
    "        \n",
    "    rows = []\n",
    "    for n, trees in all_solutions.items():\n",
    "        for i, t in enumerate(trees):\n",
    "            rows.append([f\"{n:03d}_{i}\", f\"s{t.center_x:.10f}\", f\"s{t.center_y:.10f}\", f\"s{t.angle:.10f}\"])\n",
    "    df_sub = pd.DataFrame(rows, columns=['id', 'x', 'y', 'deg']).sort_values('id')\n",
    "    df_sub.to_csv('submission.csv', index=False)\n",
    "    print(\"Pipeline finished. submission.csv generated.\")\n",
    "\n",
    "run_pipeline()"
   ]
