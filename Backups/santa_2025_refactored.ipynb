    "def validate_no_collisions(trees, verbose=False, tolerance=1e-9):\n",
    "    n = len(trees)\n",
    "    if n <= 1: return True, 0\n",
    "    polygons = [t.polygon for t in trees]\n",
    "    tree_index = STRtree(polygons)\n",
    "    collision_count = 0\n",
    "    for i, poly in enumerate(polygons):\n",
    "        query_indices = [j for j in tree_index.query(poly) if j > i]\n",
    "        for j in query_indices:\n",
    "            if poly.intersects(polygons[j]):\n",
    "                intersection_area = poly.intersection(polygons[j]).area\n",
    "                if intersection_area > tolerance:\n",
    "                    collision_count += 1\n",
    "                    if verbose: print(f\"  - Trees {i} and {j} overlap (area={intersection_area:.4e})\")\n",
    "    return collision_count == 0, collision_count\n",
    "\n",
    "print(\"Optimized collision validation function defined.\")"
   ]
